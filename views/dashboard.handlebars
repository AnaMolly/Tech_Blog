<div>
    <h1> Your Posts </h1>
    {{#each posts as |post|}}
    <div class="card" style="width: 18rem;" id={{post.id}}>
        <div class="card-body">
            <h5 class="card-title">{{post.post_title}}</h5>
            <p class="card-text">{{post.post_content}}</p>
            <p>{{post.createdAt}}</p>
            <a href="#" class="btn btn-primary">Edit Post</a>
        </div>
    </div>
    {{/each}}
     <a href="">Add New Post</a>
</div>