<ul class="post-list">
     {{#each posts as |post|}}
        <div class="card" style="width: 18rem;" id={{post.id}}>
            <div class="card-body">
            <h5 class="card-title">{{post.post_title}}</h5>
            <p class="card-text">{{post.post_content}}</p>
            <p>{{post.createdAt}}</p>
        </div>
    </div>
    {{/each}}
</ul>
